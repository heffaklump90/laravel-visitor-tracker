{
    "name": "heffaklump90/laravel-visitor-tracker",
    "description": "Visitor tracker and statistics for Laravel 5",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Zavyalov",
            "email": "alex@voerro.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.2",
        "illuminate/support": "5.5.* | 5.6.* | 5.7.* | ^11.0 ",
        "illuminate/http": "5.5.* | 5.6.* | 5.7.*| ^11.0 ",
        "illuminate/database": "5.5.* | 5.6.* | 5.7.*| ^11.0 ",
        "illuminate/bus": "5.5.* | 5.6.* | 5.7.*| ^11.0 ",
        "illuminate/queue": "5.5.* | 5.6.* | 5.7.*| ^11.0 ",
        "illuminate/contracts": "5.5.* | 5.6.* | 5.7.*| ^11.0 ",
        "matomo/device-detector": "^6.4",
        "guzzlehttp/guzzle": "~6.0 | ^7.1"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Voerro\\Laravel\\VisitorTracker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Voerro\\Laravel\\VisitorTracker\\VisitorTrackerServiceProvider"
            ],
            "aliases": {
                "VisitStats": "Voerro\\Laravel\\VisitorTracker\\Facades\\VisitStats"
            }
        }
    }
}
